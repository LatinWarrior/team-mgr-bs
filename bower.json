{
  "name": "team-mgr-bs",
  "description": "Modular AngularJS with Angm-generator",
  "version": "0.0.1",
  "install": {
    "path": {
      "js": "src/app/assets/libs",
      "css": "src/app/assets/css"
    }
  },
  "dependencies": {
    "json3": "3.3.1",
    "es5-shim": "3.1.0",
    "bootstrap": "3.3.6",
    "jquery": "2.2.0",
    "angular": ">=1.3.*",
    "angular-resource": ">=1.3.*",
    "angular-aria": ">=1.3.*",
    "angular-mocks": ">=1.3.*",
    "angular-cookies": ">=1.3.*",
    "angular-animate": ">=1.3.*",
    "angular-touch": ">=1.3.*",
    "angular-sanitize": ">=1.3.*",
    "angular-ui-router": "~0.2.10",
    "angular-bootstrap": "~0.14.3",
    "lodash": "^4.11.1"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.min.css",
        "lib/bootstrap/less/bootstrap.less"
      ]
    },
    "font-awesome": {
      "main": [
        "less/font-awesome.less",
        "css/font-awesome.min.css",
        "scss/font-awesome.scss"
      ]
    }
  },
  "resolutions": {
    "angular": ">=1.3.*"
  },
  "homepage": "https://github.com/LatinWarrior/team-mgr-bs",
  "authors": [
    "Luis <luis.f.blanco@gmail.com>"
  ],
  "main": "index.html",
  "keywords": [
    "AngularJS"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "src/bower_components",
      "src/app/lib",
    "test",
    "tests"
  ]
}
